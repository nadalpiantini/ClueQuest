{
  "name": "cluequest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5173",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "optimize:check": "npm run type-check && npm run lint",
    "setup:all": "node scripts/setup-all.js",
    "setup:dns": "node scripts/configure-empleaido-dns.js",
    "setup:email": "node scripts/configure-resend.js",
    "setup:vercel": "node scripts/configure-vercel.js",
    "setup:check": "node scripts/check-status.js",
    "db:migrate": "supabase db migrate",
    "db:reset": "supabase db reset",
    "db:optimize": "node scripts/deploy-performance-indexes.js",
    "db:monitor": "node scripts/monitor-performance.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "node scripts/test-auth-flow.js",
    "test:performance": "node scripts/test-database-performance.js",
    "test:e2e": "playwright test",
    "test:e2e:auth": "playwright test tests/e2e/auth.spec.ts",
    "test:e2e:mobile": "playwright test --project=mobile",
    "test:e2e:debug": "playwright test --headed --debug",
    "security:audit": "npm audit",
    "security:validate": "node scripts/security-validation.js",
    "production:health": "node scripts/production-health-check.js",
    "production:deploy": "npm run build && npm run optimize:check",
    "production:monitor": "node scripts/production-monitor.js",
    "analyze": "cross-env ANALYZE=true next build",
    "deploy:empleaido": "node scripts/deploy-empleaido.js",
    "deploy:quick": "npm run build && npm run deploy:empleaido",
    "security:harden": "node scripts/security-hardening.js",
    "security:monitor": "node scripts/security-monitor.js",
    "security:pentest": "node scripts/penetration-testing.js",
    "security:pentest:full": "npm run security:validate && npm run security:pentest",
    "security:compliance": "node scripts/compliance-audit.js",
    "security:soc2": "node scripts/compliance-audit.js --framework=soc2",
    "security:gdpr": "node scripts/compliance-audit.js --framework=gdpr",
    "security:owasp": "node scripts/compliance-audit.js --framework=owasp",
    "security:full": "npm run security:audit && npm run security:validate && npm run security:pentest && npm run security:compliance",
    "security:pre-deploy": "npm run security:validate && npm run security:pentest",
    "security:post-deploy": "npm run security:monitor && npm run security:compliance",
    "security:dev": "npm run security:validate && npm run security:monitor",
    "security:ci": "npm run security:audit && npm run security:validate",
    "security:update-deps": "npm audit fix && npm update",
    "security:check-deps": "npm audit --audit-level moderate",
    "security:docs": "node scripts/generate-security-docs.js",
    "security:report": "npm run security:full && node scripts/generate-security-report.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.2",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.62.7",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "ioredis": "^5.7.0",
    "jose": "^5.9.6",
    "lucide-react": "^0.468.0",
    "nanoid": "^5.0.9",
    "next": "^15.1.6",
    "openai": "^5.18.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.2",
    "resend": "^4.0.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cluequest.git"
  },
  "keywords": [
    "saas",
    "nextjs",
    "typescript",
    "supabase",
    "tailwindcss",
    "global-market",
    "production-ready"
  ],
  "author": "ClueQuest Team",
  "license": "MIT"
}
